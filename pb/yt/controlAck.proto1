syntax = "proto3";

package yt;

message ControlAck {
    uint32 protocolType = 1;
    uint32 userID       = 2;
    uint32 topicID      = 3;
    uint32 ackCode      = 4;    
}
enum protocolAckType {
    CONNECTACK        = 0;
    SUBaCK            = 1;
    UNSUBACK          = 2;
    HOLDMICACK        = 3;
    RELEASEMICACK     = 4;
    DISCONNECTACK     = 5;
}